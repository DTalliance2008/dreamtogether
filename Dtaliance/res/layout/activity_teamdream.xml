            android:layout_below="@+id/tv_teamdream_myprogress" 
    		android:text="88%"
    		android:textColor="#ff6666"
    		android:layout_marginTop="7dp"
    		android:layout_marginRight="5dp"
    		/>  
     
 <!--横线一 -->   
    <TextView 
    		android:id="@+id/tv_teamdream_linethree"
    		android:layout_width="match_parent"
