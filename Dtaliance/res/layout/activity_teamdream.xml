    		android:layout_height="2dp"
    		android:layout_marginTop="5dp"
    		android:layout_below="@+id/tv_teamdream_mytask"
    	  	android:background="@drawable/homepage_line"  />   
   <!--团队的比梦台一 -->       
     <Button 
    android:id="@+id/tv_teamdream_myteambattle"
    android:background="@drawable/homepage_tv"
    android:layout_width="80dp"
    android:layout_height="80dp"
    android:layout_marginTop="5dp"
    android:layout_marginLeft="5dp"
    android:text="我的团队比梦台"
    android:textSize="18sp"
    android:gravity="center"
    android:layout_below="@+id/tv_teamdream_linetwo" 
        />
     <TextView 
    android:id="@+id/tv_teamdream_mypro"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginLeft="20dp"
    android:layout_marginTop="20dp"
    android:textSize="16sp"
    android:layout_toRightOf="@+id/tv_teamdream_myteambattle"
    android:layout_below="@+id/tv_teamdream_linetwo" 
    android:text="我方团队进度："
        />
      <TextView 
    		android:id="@+id/tv_teamdream_myprogress"
    		android:layout_width="match_parent"
    		android:layout_height="wrap_content"
    		android:background="@drawable/homepage_tv" 
    		android:textSize="14sp"
    		android:layout_toRightOf="@+id/tv_teamdream_mypro"
            android:layout_below="@+id/tv_teamdream_linetwo" 
    		android:text="34%"
    		android:textColor="#ff6666"
    		android:layout_marginTop="22dp"
    		android:layout_marginRight="5dp"
    		/>  
     <TextView 
    android:id="@+id/tv_teamdream_enemypro"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginLeft="20dp"
    android:layout_marginTop="5dp"
    android:textSize="16sp"
    android:layout_toRightOf="@+id/tv_teamdream_myteambattle"
    android:layout_below="@+id/tv_teamdream_myprogress" 
    android:text="对手团队进度："
        />
      <TextView 
    		android:id="@+id/tv_teamdream_enemyproprogress"
