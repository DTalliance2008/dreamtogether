  <!--我的任务一 -->       
     <TextView 
    android:id="@+id/tv_teamdream_mytask"
    android:background="@drawable/homepage_tv"
    android:layout_width="80dp"
    android:layout_height="80dp"
    android:layout_marginTop="5dp"
    android:layout_marginLeft="5dp"
    android:text="团队任务"
    android:textSize="18sp"
    android:gravity="center"
    android:layout_below="@+id/tv_teamdream_lineone" 
        />
     <TextView 
    android:id="@+id/tv_teamdream_taskone"
    android:layout_width="match_parent"
