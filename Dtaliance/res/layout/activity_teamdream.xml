<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
   >

     <!--横线一 -->   
    <TextView 
    		android:id="@+id/tv_teamdream_line"
    		android:layout_width="match_parent"
    		android:layout_height="2dp"
    		android:layout_marginTop="5dp"
    	  	android:background="@drawable/homepage_line"     		
       		  />
    <!--头像一 -->   
 <TextView 
    android:id="@+id/tv_teamdream_teamicon"
    android:layout_width="80dp"
    android:layout_height="80dp"
    android:layout_marginTop="5dp"
    android:layout_marginLeft="5dp"
    android:background="@drawable/teamicon"
    android:layout_below="@+id/tv_teamdream_line" 
        />
     <TextView 
    android:id="@+id/tv_teamdream_name"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="20dp"
    android:layout_marginTop="5dp"
    android:textSize="18sp"
    android:layout_toRightOf="@+id/tv_teamdream_teamicon"
    android:layout_below="@+id/tv_teamdream_line" 
    android:text="武林第一联盟"
        />
<TextView 
    android:id="@+id/tv_teamdream_terminaldreamm"
    android:layout_width="wrap_content"
    android:layout_height="20dp"
    android:layout_marginLeft="20dp"
    android:gravity="left"
    android:textSize="16sp"
    android:layout_toRightOf="@+id/tv_teamdream_teamicon"
    android:layout_below="@+id/tv_teamdream_name" 
    android:text="最终梦想："
        />
<TextView 
    android:id="@+id/tv_teamdream_terminaldream"
    android:background="@drawable/homepage_bt"
    android:layout_width="wrap_content"
    android:layout_height="20dp"
    android:gravity="left"
    android:textSize="16sp"
    android:textColor="#ff9900"
    android:singleLine="true"
    android:ellipsize="end"
    android:layout_toRightOf="@+id/tv_teamdream_terminaldreamm"
    android:layout_below="@+id/tv_teamdream_name" 
    android:text="我想成为一名歌唱家"
        />
<TextView 
    android:id="@+id/tv_teamdream_middledreamm"
    android:layout_width="wrap_content"
    android:layout_height="20dp"
    android:layout_marginLeft="20dp"
    android:textSize="16sp"
    android:gravity="left"
    android:layout_toRightOf="@+id/tv_teamdream_teamicon"
    android:layout_below="@+id/tv_teamdream_terminaldream" 
    android:text="中期梦想："
        />
<TextView 
    android:id="@+id/tv_teamdream_middledream"
    android:background="@drawable/homepage_bt"
    android:layout_width="wrap_content"
    android:layout_height="20dp"
    android:textSize="16sp"
    android:textColor="#ff9900"
    android:singleLine="true"
    android:ellipsize="end"
    android:gravity="left"
    android:layout_toRightOf="@+id/tv_teamdream_middledreamm"
    android:layout_below="@+id/tv_teamdream_terminaldream" 
    android:text="会唱一首歌"
        />
<TextView 
    android:id="@+id/tv_teamdream_firstdreamt"
    android:layout_width="wrap_content"
    android:layout_height="20dp"
    android:layout_marginLeft="20dp"    
    android:gravity="left"
    android:textSize="16sp"
    android:layout_toRightOf="@+id/tv_teamdream_teamicon"
    android:layout_below="@+id/tv_teamdream_middledream" 
    android:text="初级梦想："
        />
<TextView 
    android:id="@+id/tv_teamdream_firstdream"
    android:background="@drawable/homepage_bt"
    android:layout_width="wrap_content"
    android:layout_height="20dp"
    android:gravity="left"
    android:textSize="16sp"
    android:singleLine="true"
    android:ellipsize="end"
    android:textColor="#ff9900"
    android:layout_toRightOf="@+id/tv_teamdream_firstdreamt"
    android:layout_below="@+id/tv_teamdream_middledream" 
    android:text="读懂五线谱"
        />
  <!--横线一 -->   
    <TextView 
    		android:id="@+id/tv_teamdream_lineone"
    		android:layout_width="match_parent"
    		android:layout_height="2dp"
    		android:layout_marginTop="5dp"
    		android:layout_below="@+id/tv_teamdream_firstdream"
