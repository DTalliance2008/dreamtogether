<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
   >

     <!--横线一 -->   
    <TextView 
    		android:id="@+id/tv_homepage_line"
    		android:layout_width="match_parent"
    		android:layout_height="2dp"
    		android:layout_marginTop="5dp"
    	  	android:background="@drawable/homepage_line"     		
       		  />
    <!--头像一 -->   
 <ImageButton 
    android:id="@+id/ib_homepage_myicon"
    android:layout_width="80dp"
    android:layout_height="80dp"
    android:layout_marginTop="5dp"
    android:layout_marginLeft="5dp"
    android:background="@drawable/dream_myicon"
    android:layout_below="@+id/tv_homepage_line" 
        />
     <TextView 
    android:id="@+id/tv_homepage_name"
    android:layout_width="match_parent"
    android:layout_height="30dp"
    android:layout_marginLeft="20dp"
    android:layout_marginTop="5dp"
    android:textSize="18sp"
    android:layout_toRightOf="@+id/ib_homepage_myicon"
    android:layout_below="@+id/tv_homepage_line" 
    android:text="天天爱睡觉"
        />
<TextView 
    android:id="@+id/tv_homepage_dreamm"
    android:layout_width="wrap_content"
    android:layout_height="40dp"
    android:layout_marginLeft="20dp"
    android:layout_marginTop="10dp"
    android:textSize="16sp"
    android:layout_toRightOf="@+id/ib_homepage_myicon"
    android:layout_below="@+id/tv_homepage_name" 
    android:text="我的梦想："
        />
<TextView 
    android:id="@+id/tv_homepage_dream"
    android:background="@drawable/homepage_bt"
    android:layout_width="wrap_content"
    android:singleLine="true"
    android:ellipsize="end"
    android:layout_marginTop="10dp"    
    android:layout_height="20dp"
    android:textSize="16sp"
    android:textColor="#ff9900"
    android:layout_toRightOf="@+id/tv_homepage_dreamm"
    android:layout_below="@+id/tv_homepage_name" 
    android:text="我想成为一名歌唱家"
        />

  <!--横线一 -->   
    <TextView 
    		android:id="@+id/tv_homepage_lineone"
    		android:layout_width="match_parent"
    		android:layout_height="2dp"
    		android:layout_marginTop="5dp"
    		android:layout_below="@+id/ib_homepage_myicon"
    	  	android:background="@drawable/homepage_line"  />
    
  <!--我的任务一 -->       
     <TextView 
    android:id="@+id/tv_homepage_mytask"
    android:background="@drawable/homepage_tv"
    android:layout_width="80dp"
    android:layout_height="80dp"
    android:layout_marginTop="5dp"
    android:layout_marginLeft="5dp"
    android:text="我的任务"
    android:textSize="18sp"
    android:gravity="center"
    android:layout_below="@+id/tv_homepage_lineone" 
        />
     <TextView 
    android:id="@+id/tv_homepage_taskone"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="20dp"
    android:layout_marginTop="5dp"
    android:textSize="16sp"
    android:textColor="#ff6666"
    android:layout_toRightOf="@+id/tv_homepage_mytask"
    android:layout_below="@+id/tv_homepage_lineone" 
    android:text="吃完一天的饭"
        />
<TextView 
    android:id="@+id/tv_homepage_tasktwo"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="20dp"
    android:layout_marginTop="3dp"
    android:textSize="16sp"
    android:textColor="#ff6666"
    android:layout_toRightOf="@+id/tv_homepage_mytask"
    android:layout_below="@+id/tv_homepage_taskone" 
    android:text="喝完一天的水"
        />
<TextView 
    android:id="@+id/tv_homepage_taskthree"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="20dp"
    android:layout_marginTop="3dp"
    android:textSize="16sp"
    android:textColor="#ff6666"
    android:layout_toRightOf="@+id/tv_homepage_mytask"
    android:layout_below="@+id/tv_homepage_tasktwo" 
    android:text="唱一天的歌"
        />
  <!--横线一 -->   
    <TextView 
    		android:id="@+id/tv_homepage_linetwo"
    		android:layout_width="match_parent"
    		android:layout_height="2dp"
    		android:layout_marginTop="5dp"
    		android:layout_below="@+id/tv_homepage_mytask"
    	  	android:background="@drawable/homepage_line"  />   
   <!--我的比梦台一 -->       
     <Button 
    android:id="@+id/bt_homepage_mybattle"
    android:background="@drawable/homepage_tv"
    android:layout_width="80dp"
    android:layout_height="80dp"
    android:layout_marginTop="5dp"
    android:layout_marginLeft="5dp"
    android:text="我的比梦台"
    android:textSize="18sp"
    android:gravity="center"
    android:layout_below="@+id/tv_homepage_linetwo" 
        />
     <TextView 
    android:id="@+id/tv_homepage_mypro"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginLeft="20dp"
    android:layout_marginTop="20dp"
    android:textSize="16sp"
    android:layout_toRightOf="@+id/bt_homepage_mybattle"
    android:layout_below="@+id/tv_homepage_linetwo" 
    android:text="我的进度："
        />
      <TextView 
    		android:id="@+id/tv_homepage_myprogress"
    		android:layout_width="match_parent"
    		android:layout_height="wrap_content"
    		android:background="@drawable/homepage_tv" 
    		android:textSize="14sp"
    		android:layout_toRightOf="@+id/tv_homepage_mypro"
            android:layout_below="@+id/tv_homepage_linetwo" 
    		android:text="34%"
    		android:textColor="#ff6666"
    		android:layout_marginTop="22dp"
    		android:layout_marginRight="5dp"
    		/>  
     <TextView 
    android:id="@+id/tv_homepage_enemypro"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginLeft="20dp"
    android:layout_marginTop="5dp"
    android:textSize="16sp"
    android:layout_toRightOf="@+id/bt_homepage_mybattle"
    android:layout_below="@+id/tv_homepage_myprogress" 
    android:text="对手进度："
        />
      <TextView 
    		android:id="@+id/tv_homepage_enemyprogress"
    		android:layout_width="match_parent"
    		android:layout_height="wrap_content"
    		android:background="@drawable/homepage_tv" 
    		android:textSize="14sp"
    		android:layout_toRightOf="@+id/tv_homepage_enemypro"
            android:layout_below="@+id/tv_homepage_myprogress" 
    		android:text="88%"
    		android:textColor="#ff6666"
    		android:layout_marginTop="7dp"
    		android:layout_marginRight="5dp"
    		/>  
     
 <!--横线一 -->   
    <TextView 
    		android:id="@+id/tv_homepage_linethree"
    		android:layout_width="match_parent"
    		android:layout_height="2dp"
    		android:layout_marginTop="5dp"
    		android:layout_below="@+id/bt_homepage_mybattle"
    	  	android:background="@drawable/homepage_line"  /> 
    	  	
    <!--       我    的   团    队     -->
      <ImageButton 
    android:id="@+id/ib_homepage_myteam"
    android:background="@drawable/teamicon"
    android:layout_width="80dp"
    android:layout_height="80dp"
    android:layout_marginTop="5dp"
    android:layout_marginLeft="5dp"
    android:textSize="18sp"
    android:gravity="center"
    android:layout_below="@+id/tv_homepage_linethree" 
        />
     <TextView 
    android:id="@+id/tv_homepage_myteamname"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="20dp"
    android:layout_marginTop="5dp"
    android:textSize="18sp"
    android:layout_toRightOf="@+id/ib_homepage_myteam"
    android:layout_toLeftOf="@+id/bt_homepage_myteamedit"
    android:layout_below="@+id/tv_homepage_linethree" 
    android:text="宇宙第一战队"
        />
     
       <Button 
    android:id="@+id/bt_homepage_myteamedit"
    android:layout_width="wrap_content"
    android:layout_height="20dp"
    android:gravity="center"
    android:layout_marginRight="5dp"
    android:layout_marginTop="5dp"
    android:textColor="#ccccff"
    android:layout_alignParentRight="true"    
    android:background="@drawable/homepage_circle"
    android:textSize="14sp"
    android:layout_below="@+id/tv_homepage_linethree" 
    android:text="编辑"
        />
     
     
      <TextView 
    		android:id="@+id/tv_homepage_myteamdreamm"
    		android:layout_width="wrap_content"
    		android:layout_height="20dp"
    		android:textSize="16sp"
    		android:layout_toRightOf="@+id/ib_homepage_myteam"
            android:layout_below="@+id/tv_homepage_myteamname" 
    		android:text="团队梦想："
    		android:gravity="left"
    		android:layout_marginTop="5dp"
   		    android:layout_marginLeft="20dp"
    		/>  
    		
      
      <TextView 
    		android:id="@+id/tv_homepage_myteamdream"
    		android:layout_width="wrap_content"
    		android:layout_height="20dp"
    		android:background="@drawable/homepage_bt" 
    		android:textSize="16sp"
    		android:layout_toRightOf="@+id/tv_homepage_myteamdreamm"
            android:layout_below="@+id/tv_homepage_myteamname" 
    		android:text="打造第一社交平台"
    		android:gravity="left"
    		android:textColor="#ff9900"
    		android:layout_marginTop="5dp"
    		/> 
     <TextView 
    android:id="@+id/tv_homepage_myteamdreamtaskpro"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginLeft="20dp"
    android:layout_marginTop="5dp"
    android:textSize="16sp"
    android:layout_toRightOf="@+id/ib_homepage_myteam"
    android:layout_below="@+id/tv_homepage_myteamdreamm" 
    android:text="团队进度："
        />
    <TextView 
    android:id="@+id/tv_homepage_myteamdreamtaskprogress"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="5dp"
    android:background="@drawable/homepage_tv"
    android:textSize="14sp"
    android:layout_toRightOf="@+id/tv_homepage_myteamdreamtaskpro"
    android:layout_below="@+id/tv_homepage_myteamdream" 
    android:text="34%"
    android:textColor="#ff6666"
    android:layout_marginRight="5dp"
        />
   <!--横线一 -->   
    <TextView 
    		android:id="@+id/tv_homepage_linefive"
    		android:layout_width="match_parent"
    		android:layout_height="2dp"
    		android:layout_marginTop="5dp"
    		android:layout_below="@+id/ib_homepage_myteam"
    	  	android:background="@drawable/homepage_line"  />   
</RelativeLayout>
